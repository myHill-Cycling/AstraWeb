{
  "name": "@myhill-cycling/astra-web",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "^16",
    "yarn": "^3"
  },
  "repository": {
    "directory": "git",
    "url": "https://github.com/myHill-Cycling/AstraWeb"
  },
  "maintainers": [
    "Vespion"
  ],
  "scripts": {
    "start": "node staging-server.cjs",
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "lint": "eslint . --ext .js,.ts.jsx,.tsx,.cjs,.mjs --ignore-path .gitignore --cache --cache-location ./.cache/eslint/",
    "lint#fix": "yarn run lint --fix",
    "cypress#open": "cypress open",
    "type-check": "tsc -b"
  },
  "dependencies": {
    "http-server": "14.1.1"
  },
  "devDependencies": {
    "@astrojs/preact": "0.1.3",
    "@astrojs/sitemap": "0.1.0",
    "@astrojs/tailwind": "0.2.1",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@types/jquery": "3.5.14",
    "@typescript-eslint/parser": "5.27.1",
    "astro": "1.0.0-beta.40",
    "astro-critters": "0.0.7",
    "astro-eslint-parser": "0.2.2",
    "astro-icon": "0.7.1",
    "astro-robots-txt": "0.3.1",
    "astro-seo": "0.5.0",
    "astro-webmanifest": "0.3.0",
    "axe-core": "4.4.2",
    "cypress": "9.7.0",
    "cypress-axe": "0.14.0",
    "dotenv": "16.0.1",
    "eslint": "8.17.0",
    "eslint-config-preact": "1.3.0",
    "eslint-plugin-compat": "4.0.2",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "flowbite": "1.4.7",
    "preact": "10.7.3",
    "semantic-release": "19.0.2",
    "typescript": "4.7.3",
    "vite-imagetools": "4.0.4"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "scope": "deps",
              "release": "minor"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md",
          "changelogTitle": "# Semantic Versioning Changelog"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json",
            "yarn.lock"
          ]
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "path": "static_site.zip",
              "name": "static_site.zip",
              "label": "Website Build"
            }
          ]
        }
      ]
    ]
  },
  "packageManager": "yarn@3.2.1"
}
