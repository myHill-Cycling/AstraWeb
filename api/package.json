{
	"name": "@myhill-cycling/astra-web.api",
	"description": "This package contains the code required to run the supporting API for myHill cycling's main web site",
	"scripts": {
		"build": "tsc",
		"watch": "tsc -w",
		"prestart": "yarn build",
		"start": "func start",
		"test": "jest --testLocationInResults",
		"type-check": "tsc-files -p ./tsconfig.eslint.json --noEmit",
		"lint": "yarn run lint#base .",
		"lint#base": "eslint --ignore-path ../.gitignore --cache --cache-location ../.cache/eslint/",
		"lint#fix": "yarn run lint --fix"
	},
	"engines": {
		"node": "^16",
		"yarn": "^3"
	},
	"dependencies": {
		"azure-functions-core-tools": "4.0.4629"
	},
	"devDependencies": {
		"@azure/functions": "3.2.0",
		"@babel/core": "7.18.6",
		"@babel/preset-env": "7.18.6",
		"@babel/preset-typescript": "7.18.6",
		"@fast-check/jest": "0.0.1",
		"@types/jest": "28.1.6",
		"@types/mailparser": "3.4.0",
		"@types/node": "18.0.0",
		"@types/nodemailer": "6.4.4",
		"@typescript-eslint/eslint-plugin": "5.30.5",
		"@typescript-eslint/parser": "5.30.5",
		"eslint": "8.20.0",
		"eslint-plugin-jest": "26.6.0",
		"fast-check": "3.0.1",
		"fastest-validator": "1.12.0",
		"jest": "28.1.3",
		"mailparser": "3.5.0",
		"node-fetch": "3.2.8",
		"nodemailer": "6.7.7",
		"tsc-files": "1.1.3",
		"typescript": "4.7.4"
	},
	"packageManager": "yarn@3.2.1"
}
