{
	"name": "@myhill-cycling/astra-web.api",
	"description": "This package contains the code required to run the supporting API for myHill cycling's main web site",
	"scripts": {
		"build": "tsc",
		"watch": "tsc -w",
		"prestart": "yarn build",
		"start": "func start",
		"test": "echo \"No tests yet...\"",
		"type-check": "tsc-files --noEmit",
		"lint": "yarn run lint#base .",
		"lint#base": "eslint --ignore-path ../.gitignore --cache --cache-location ../.cache/eslint/",
		"lint#fix": "yarn run lint --fix"
	},
	"engines": {
		"node": "^16",
		"yarn": "^3"
	},
	"dependencies": {
		"azure-functions-core-tools": "4.0.4629"
	},
	"devDependencies": {
		"@azure/functions": "3.2.0",
		"@types/node": "18.0.0",
		"@types/nodemailer": "6.4.4",
		"@typescript-eslint/eslint-plugin": "5.30.5",
		"@typescript-eslint/parser": "5.30.5",
		"eslint": "8.19.0",
		"fastest-validator": "1.12.0",
		"nodemailer": "6.7.7",
		"tsc-files": "1.1.3",
		"typescript": "4.7.4"
	},
	"packageManager": "yarn@3.2.1"
}
