{
	"masterPicture": "./assets/images/favi/favicon.png",
	"iconsPath": "/img/favi/",
	"design": {
		"ios": {
			"pictureAspect": "backgroundAndMargin",
			"backgroundColor": "#ffffff",
			"margin": "11%",
			"assets": {
				"ios6AndPriorIcons": false,
				"ios7AndLaterIcons": false,
				"precomposedIcons": false,
				"declareOnlyDefaultIcon": true
			},
			"appName": "myHill Cycling"
		},
		"desktopBrowser": {
			"design": "raw"
		},
		"windows": {
			"pictureAspect": "noChange",
			"backgroundColor": "#f70702",
			"onConflict": "override",
			"assets": {
				"windows80Ie10Tile": false,
				"windows10Ie11EdgeTiles": {
					"small": false,
					"medium": true,
					"big": false,
					"rectangle": false
				}
			},
			"appName": "myHill Cycling"
		},
		"androidChrome": {
			"pictureAspect": "noChange",
			"themeColor": "#f70702",
			"manifest": {
				"name": "myHill Cycling",
				"display": "browser",
				"orientation": "notSet",
				"onConflict": "override",
				"declared": true
			},
			"assets": {
				"legacyIcon": true,
				"lowResolutionIcons": false
			}
		},
		"safariPinnedTab": {
			"pictureAspect": "blackAndWhite",
			"threshold": 54.6875,
			"themeColor": "#f70702"
		}
	},
	"settings": {
		"compression": 4,
		"scalingAlgorithm": "Mitchell",
		"errorOnImageTooSmall": false,
		"readmeFile": false,
		"htmlCodeFile": false,
		"usePathAsIs": false
	},
	"versioning": {
		"paramName": "v",
		"paramValue": "2"
	}
}